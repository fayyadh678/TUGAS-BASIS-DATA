1.CREATE TABLE DEPARTMENT (
    DNO INT PRIMARY KEY,
    DNAME VARCHAR(50),
    DMGRSSN VARCHAR(15)
);
CREATE TABLE DEPARTMENT_LOCATIONS (
    DNO INT,
    DLOCATIONS VARCHAR(50),
    PRIMARY KEY (DNO, DLOCATIONS),
    FOREIGN KEY (DNO) REFERENCES DEPARTMENT(DNO)
);


2.CREATE TABLE EMP_PROJ (
    SSN VARCHAR(15) PRIMARY KEY,
    ENAME VARCHAR(50),
    PNO VARCHAR(50),       -- Menyimpan beberapa proyek dalam satu kolom
    HOURS VARCHAR(50)      -- Menyimpan jam kerja untuk beberapa proyek dalam satu kolom
);
CREATE TABLE EMP_PROJ (
    SSN VARCHAR(15),
    ENAME VARCHAR(50),
    PNO INT,
    HOURS DECIMAL(5, 2),
    PRIMARY KEY (SSN, PNO)
);
